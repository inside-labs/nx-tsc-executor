    "@types/nodemon": "^1.19.0",
    "@types/rimraf": "^2.0.3",
    "@types/shelljs": "^0.8.6",
    "husky": "^4.3.6",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1100.5",
    "@angular-devkit/core": "^11.0.5",
    "concurrently": "^5.3.0",
    "cpx": "^1.5.0",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }

  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "builders": "builders.json",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",

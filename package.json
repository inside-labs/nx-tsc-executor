    {
      "path": "dist/nx-tsc-executor.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/concurrently": "^4.1.0",
    "@types/cpx": "^1.5.1",
